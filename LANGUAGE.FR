BOT1_INTRO_TXT1 = "Bienvenue au paradis !";
BOT1_INTRO_TXT2 = "Que la fête commence !.";
BOT1_INTRO_TXT3 = "Il est temps de s'amuser un peu !";
BOT1_INTRO_TXT4 = "Super,encore ce foutu trou à rats !";
