BOT1_ENRAGED_TXT1 = "VAS TE FAIRE FOUTRE, CONNARD !";
BOT1_ENRAGED_TXT2 = "VOUS ALLEZ SUBIR DE LA TORTURE !!";
BOT1_ENRAGED_TXT3 = "JE VAIS TOUS VOUS ENCULER !!";
